{
  "name": "@qnpm/ykit",
  "version": "0.1.6-rc0",
  "description": "高可用高扩展前端开发工具",
  "main": "src/ykit.js",
  "bin": {
    "ykit": "bin/ykit"
  },
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register --timeout 0",
    "start": "cd .. && sudo node ykit/bin/ykit server"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "colors": "^1.1.2",
    "connect": "^3.4.1",
    "css-loader": "^0.23.1",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "globby": "^5.0.0",
    "html-loader": "^0.4.3",
    "inquirer": "^1.1.1",
    "jerryproxy-ykit": "^1.0.66",
    "js-yaml": "^3.6.1",
    "json-loader": "^0.5.4",
    "json5": "^0.5.0",
    "left-pad": "^1.1.3",
    "loader-utils": "^0.2.15",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "optimist": "^0.6.1",
    "replacestream": "^4.0.0",
    "request": "^2.75.0",
    "require-uncached": "^1.0.2",
    "requireg": "^0.1.5",
    "right-pad": "^1.0.1",
    "semver": "^5.3.0",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "style-loader": "^0.13.1",
    "stylelint": "^6.9.0",
    "through2": "^2.0.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-md5-hash": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.corp.qunar.com:mfe/ykit.git"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "findup": "^0.1.5",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-watch": "^4.3.8",
    "mocha": "^3.1.2",
    "ps-tree": "^1.1.0",
    "semver-regex": "^1.0.0",
    "shelljs": "^0.7.5"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint ./src",
      "commit-msg": "node ./validate-commit-msg.js"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match111 subject pattern!",
      "helpMessage": ""
    }
  }
}
